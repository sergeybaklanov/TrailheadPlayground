<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Is_Billable__c</fullName>
    <description>if Dev has at least one billable project assigned - green image, else - yellow</description>
    <externalId>false</externalId>
    <formula>IF(Total_Billable_Projects__c &gt;0,IMAGE(&quot;/resource/Green&quot;,&quot;Green&quot;,20,20),IMAGE(&quot;/resource/Yellow&quot;,&quot;Yellow&quot;,20,20))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Is Billable?</label>
    <required>false</required>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
